{
    "name": "atompixel",
    "version": "1.0.0",
    "description": "AtomPixel is a admin and client dashboard for view and administrate the share photo gallery",
    "main": "server.js",
    "scripts": {
        "client": "cd client && npm start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others \"npm run dev\" \"npm run server\"",
        "format": "prettier --write ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gobinathvaratharajan/atompixel.git"
    },
    "keywords": [
        "node",
        "react"
    ],
    "author": "Gobinath Varatharajan",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gobinathvaratharajan/atompixel/issues"
    },
    "homepage": "https://github.com/gobinathvaratharajan/atompixel#readme",
    "dependencies": {
        "bcrypt": "^5.1.1",
        "chalk": "^5.4.1",
        "config": "^3.3.12",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "knex": "^3.1.0",
        "mysql2": "^3.12.0",
        "uuid": "^11.0.5"
    },
    "devDependencies": {
        "concurrently": "^9.1.2",
        "nodemon": "^3.1.9",
        "prettier": "^3.4.2"
    },
    "nodemonConfig": {
        "ignore": [
            "client/",
            "config/",
            "server.js",
            "knexfile.js"
        ]
    }
}
