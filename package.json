{
  "name": "atompixel",
  "version": "1.0.0",
  "description": "AtomPixel is a admin and client dashboard for view and administrate the share photo gallery",
  "main": "server.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others \"npm run client\" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gobinathvaratharajan/atompixel.git"
  },
  "keywords": [
    "node",
    "react"
  ],
  "author": "Gobinath Varatharajan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gobinathvaratharajan/atompixel/issues"
  },
  "homepage": "https://github.com/gobinathvaratharajan/atompixel#readme",
  "dependencies": {
    "chalk": "^5.4.1",
    "config": "^3.3.12",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "knex": "^3.1.0",
    "mysql2": "^3.12.0"
},
"devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9"
  },
  "nodemonConfig": {
    "ignore": [
        "client/",
        "config/",
        "server.js",
        "knexfile.js"
    ]
  }
}
